# Configuration npm pour le projet
registry=https://registry.npmjs.org/

# Désactiver les messages de financement pour un output plus propre
fund=false

# Désactiver les suggestions de mise à jour
update-notifier=false

# Améliorer la fiabilité des téléchargements
fetch-retries=5
fetch-retry-mintimeout=20000
fetch-retry-maxtimeout=120000
fetch-timeout=300000
