{
  "name": "one-piece-booster-game",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "overrides": {
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.5.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "download-vegapull-data": "node scripts/download-vegapull-data.cjs",
    "download-images": "node scripts/download-images-only.cjs",
    "download-sample-images": "node scripts/download-sample-images.cjs",
    "download-zip-images": "node scripts/download-zip-images.cjs",
    "create-image-index": "node scripts/create-image-index.cjs",
    "build-vegapull": "cd vegapull && cargo build --release",
    "setup-images": "npm run build-vegapull && npm run download-images",
    "setup-vegapull": "npm run download-vegapull-data"
  },
  "dependencies": {
    "lucide-react": "^0.469.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router-dom": "^7.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/react": "^19.0.9",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.17",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "vite-plugin-pwa": "^1.1.0"
  }
}
